{% extends "admin/base.html.twig" %}
{% block title %}
	{{ parent () }}
	Dashboard
{% endblock %}


{% block body %}
	<title>Dashboard - userController</title>
	<table class = "table">
		<tr>
            <th>Nom</th>
		    <th>Prenom</th>
		    <th>Adresse email</th>
        </tr>
	{% for user in users %}
		<tr>
			<td>{{user.name}}</td>
			<td>{{user.firstName}}</td>
			<td>{{user.email}}</td>

		</tr>
	{% endfor %}
</table>{% endblock %}
